{
  "name": "noteli",
  "description": "A CLI based Notes App built using TypeScript, MongoDB and Auth0.",
  "version": "1.2.9",
  "author": "sahilpabale @sahilpabale",
  "bin": {
    "noteli": "./bin/run"
  },
  "bugs": "https://github.com/sahilpabale/noteli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "figchalk": "^1.0.3",
    "figlet": "^1.5.2",
    "inquirer": "^8.1.2",
    "mongoose": "^5.13.7",
    "node-notifier": "^10.0.0",
    "open": "^8.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^7.3.3",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/.env",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/sahilpabale/noteli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "noteli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>. <%= chalk.yellowBright(`\nJust run: npm update -g noteli`)%>"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sahilpabale/noteli.git"
  },
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc --build && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
